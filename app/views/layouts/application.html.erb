<!DOCTYPE html>
<html>
<head>
  <title>TvNotifier</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if session[:user_id] %>
<p>Logged in</p>

	<%= button_to "Logout", logout_path, method: :delete %>
<% end %>

<%= yield %>

</body>
</html>
